{
	"compilerOptions": {
        "module": "esNext",
		"target": "es2021",
		"moduleResolution": "node",
		"strict": true,
		"noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "skipLibCheck": true,
		"preserveConstEnums": true,
        "sourceMap": true,
        "strictPropertyInitialization": false,
        "rootDir": "src"
	},
    "exclude": ["node_modules", "ise", "bin", "obj", "kite"]
}